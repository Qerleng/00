{
  "rules": [
    {
      "domain": [
        "time.windows.com"
      ]
    },
    {
      "domain_keyword": [
        "ntp",
        "time"
      ]
    },
    {
      "domain_regex": [
        "ntp.*.com",
        "ntp[1-7].*.com",
        "time.*.com",
        "time[1-7].*.com"
      ]
    },
    {
      "domain_suffix": [
        ".ntp.org"
      ]
    },
    {
      "source_port_range": [
        "123",
        "136",
        "137",
        "138",
        "139"
      ]
    }
  ],
  "version": 2
}